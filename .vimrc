" Basic {
    set nocompatible
    set background=dark
    set title
" }

" Coding {
    syntax on
    set encoding=utf8
    set softtabstop=4
    set tabstop=4
    set shiftwidth=4
    set autoindent
    set expandtab
    set nowrap
    set ruler
    set showmatch
    set number
    set numberwidth=5
" }

